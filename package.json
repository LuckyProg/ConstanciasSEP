{
  "name": "constancias",
  "version": "1.0.0",
  "description": "Programa generador de archivos XML para constancias de la SEP",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "packosx": "electron-packager . $npm_package_productName --out=dist/osx --platform=darwin --arch=x64 --icon=assets/build/osx/icon.icns && npm run codesign",
    "packwin32": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=ia32",
    "packwin64": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=x64 --version=0.36.2 app-version=1.0 --icon=assets/build/win/icon.ico",
    "build": "npm run packosx && npm run packwin32 && npm run packwin64"
  },
  "author": "derdavid",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.0",
    "electron-packager": "^12.1.2"
  }
}
